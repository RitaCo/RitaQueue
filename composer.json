{
    "name": "ritaco/JobQueue",
    "description": "JobQueue plugin for CakePHP",
    "type": "cakephp-plugin",
    "homepage": https://github.com/RitaCo/cakephp-job-queue",
    "license": "MIT",
    "keywords": ["cakephp3",  "Job", "task", "queue"],
    "version": "3.0.0-dev",
    "authors": [
        {
            "name": "Saleh Souzanchi",
            "role": "Author",
            "homepage": "http://www.ritaco.net"
        }
    ],
    "support":{
        "source": "https://github.com/RitaCo/cakephp-job-queue",
        "issues": "https://github.com/RitaCo/cakephp-job-queue/issues"
    },
    "require": {
        "php": ">=5.4",
        "cakephp/cakephp": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Rita\\QueueJob\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rita\\QueueJob\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "extra": {
        "installer-name": "Rita/JobQueue"
    }           
}
